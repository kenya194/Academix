{
  "expo": {
    "name": "Academix",
    "slug": "Academix",
    "entryPoint": "./App.js",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "scheme": "academix",
    "jsEngine": "jsc",
    "assetBundlePatterns": [
      "**/*"
    ],
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "ios": {
      "supportsTablet": true,
      "jsEngine": "hermes",
      "bundleIdentifier": "com.astromyllc.academix",
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "academix"
            ]
          }
        ]
      }
    },
    "android": {
      "package": "com.astromyllc.academix",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "academix",
              "host": "oauthredirect"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "permissions": [
        "android.permission.INTERNET"
      ]
    },
    "plugins": [
      "expo-secure-store",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ],
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "extra": {
      "eas": {
        "projectId": "27f28cea-8c7d-4059-b312-150b61f7f1b6"
      }
    }
  }
}
